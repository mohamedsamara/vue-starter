{
  "name": "vue-starter",
  "version": "1.0.0",
  "description": "Vue starter",
  "main": "src/main.js",
  "scripts": {
    "clean": "rimraf ./build",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --mode=development --config webpack/webpack.dev.js",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --mode=production  --config webpack/webpack.prod.js --color --progress",
    "serve": "serve --single build",
    "lint": "eslint '**/*.{js,vue}' --ignore-path .eslintignore",
    "lint:fix": "eslint '**/*.{js,vue}' --fix --ignore-path .eslintignore",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohamedsamara/vue-starter.git"
  },
  "author": "Mohamed Samara (https://github.com/mohamedsamara)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mohamedsamara/vue-starter/issues"
  },
  "homepage": "https://github.com/mohamedsamara/vue-starter#readme",
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^5.1.2",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "deepmerge": "^4.2.2",
    "dotenv-webpack": "^1.8.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-vue": "^7.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^0.11.2",
    "node-sass": "^5.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "rimraf": "^3.0.2",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "serve": "^11.3.2",
    "style-loader": "^1.2.1",
    "vue-eslint-parser": "^7.3.0",
    "vue-loader": "^15.9.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.4"
  },
  "dependencies": {
    "vue": "^2.6.12",
    "vue-router": "^3.4.9",
    "vuetify": "^2.3.19"
  }
